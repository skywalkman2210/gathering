@using Gathering.Models
@{
    ViewBag.Title = "Dashboard";
}
<h2>@ViewBag.Title</h2>

<head>
    <link href="@Url.Content("~/Content/Grades.css")" rel="stylesheet" type="text/css" />
</head>
<div id="tileContainer">
    @foreach (Course course in ViewBag.Courses)
    {
        <a href="@Url.Action("Index", "Grade", new {Id = course.Id})">
            <div class="tile" id="tile @course.Id">
                <p>@course.Name</p>
                <p> Period Number / Section Number </p>
            </div>
        </a>
    }
</div>
