@{
    ViewBag.Title = "Grades";
}
<h2>@ViewBag.Title</h2>

<head>
    <link href="@Url.Content("~/Content/Grades.css")" rel="stylesheet" type="text/css" />
</head>
<div id="tileContainer">
    @{ 
            int[] tiles = { 1, 2, 3, 4, 5 };
            foreach (var tile in tiles)
            {
                @Html.Raw("<div id=\"tile\" class=\"tile\">" +
                    "<p> Class Name </p>" +
                    "<p> Period Number / Section Number </p>" +
                "</div>")
            }
    }
</div>
